{
  "name": "travian-status",
  "version": "0.1.0",
  "description": "Tranvaian Status",
  "productName": "Travian Status",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "prepare": "husky",
    "build": "quasar build",
    "build:offline": "cross-env BUILD_TYPE=offline quasar build"
  },
  "lint-staged": {
    "src/**/*.{js,ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{scss,html,md,json}": "prettier --write"
  },
  "dependencies": {
    "@monaco-editor/loader": "^1.3.3",
    "@quasar/extras": "^1.16.17",
    "axios": "^1.6.2",
    "d3": "^7.9.0",
    "markdown-it": "^13.0.2",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-sup": "^1.0.0",
    "monaco-editor": "^0.41.0",
    "nanoid": "^4.0.0",
    "pinia": "^2.0.22",
    "quasar": "^2.17.7",
    "semver": "^7.6.2",
    "vue": "^3.2.39",
    "vue-router": "^4.1.5",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.11.0",
    "@quasar/cli": "^2.4.1",
    "@types/markdown-it": "^13.0.2",
    "@types/markdown-it-link-attributes": "^3.0.3",
    "@types/node": "^18.7.19",
    "@types/semver": "^7.5.8",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@unocss/vite": "^0.45.22",
    "autoprefixer": "^10.4.12",
    "cross-env": "^7.0.3",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.5.1",
    "husky": "^9.0.11",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.8.3",
    "unocss": "^0.45.22"
  },
  "engines": {
    "node": "^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
